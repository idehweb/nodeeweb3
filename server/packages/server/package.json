    "date-fns": "^2.28.0",
    "deep-equal": "^2.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.18.1",
    "express-sitemap-xml": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "isbot": "^3.4.6",
