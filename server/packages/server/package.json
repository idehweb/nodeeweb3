    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "request-ip": "^3.3.0",
    "shelljs": "^0.8.5",
    "typescript": "^4.9.3",
    "useragent": "^2.3.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "babel-version": "babel-node --version",
    "wizard": "node wizard",
    "start": "set BABEL_CACHE_PATH=./node_modules/babel-cache.json && babel-node ./src/bin/www.js"
  },
  "bin":"./cli.js",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@craco/craco": "^6.4.3",
    "define-variable-webpack-plugin": "^1.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "fs": "^0.0.1-security",
    "ftp-deploy": "^2.4.2",
    "nodemon": "^2.0.16",
    "sass": "^1.56.1"
  },
  "imports": {
    "#json/*": "./src/*.json",
    "#v/*": "./*.js",
    "#root/*": "./src/*.mjs",
    "#models/*": "./src/models/*.mjs",
    "#controllers/*": "./src/controllers/*.mjs",
    "#routes/*": "./src/routes/*.mjs",
    "#assets/*": "./src/client/assets/*",
    "#c/*": "./src/client/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "import/no-anonymous-default-export": 0,
      "prettier/prettier": 0,
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ]
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "semi": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://idehweb.com/product/nodeeweb",
  "keywords": [
    "core",
    "nodeeweb core",
    "express",
    "cms",
    "api",
    "page builder",
    "create website",
    "create application",
    "react ssr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idehweb/nodeeweb-server"
  },
  "bugs": {
    "url": "https://github.com/idehweb/nodeeweb-server/issues"
  },
  "main": "index.mjs"
}
