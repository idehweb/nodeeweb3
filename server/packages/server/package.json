    "mime": "^3.0.0",
    "moment-jalaali": "^0.9.6",
    "mongoose": "^6.3.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "openai": "^3.1.0",
    "persianjs": "^0.4.0",
    "pkg-dir": "^7.0.0",
    "pkg-up": "^4.0.0",
